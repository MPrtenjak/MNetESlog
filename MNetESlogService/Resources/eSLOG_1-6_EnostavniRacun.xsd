		<xs:simpleType>
			<xs:restriction base="xs:float">
				<xs:pattern value="([0-9]|\+|\-|E|e|\.){0,18}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="PoljubnoBesediloRazsirjeno">
		<xs:annotation>
			<xs:documentation>Razsirjeno poljubno besedilo racuna (E:Segment-FTX)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="VrstaBesedila"/>
				<xs:element ref="ImeBesedila"/>
				<xs:element ref="VrednostBesedila"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="ImeBesedila">
		<xs:annotation>
			<xs:documentation>Ime rasirjenega poljubnega besedila</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="70"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="VrednostBesedila">
		<xs:annotation>
			<xs:documentation>Vrednost razsirjenega poljubnega besedila </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="Tabela" maxOccurs="unbounded"/>
				<xs:element ref="Tekst" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="Tabela">
		<xs:annotation>
			<xs:documentation>Tabela razsirjenega poljubnega besedila</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="xs:string"/>
				<xs:element name="Row" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Field" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="Name" type="xs:string" use="required"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="Number" type="xs:string" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="Tekst">
		<xs:annotation>
			<xs:documentation>Poljuben tekst (E:El-4440)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="70"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
