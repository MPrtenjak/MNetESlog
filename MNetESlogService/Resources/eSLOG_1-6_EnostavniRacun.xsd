		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="BIC">
		<xs:annotation>
			<xs:documentation>BIC koda</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="11"/>
				<xs:pattern value="[A-Z]{6,6}[A-Z2-9][A-NP-Z|0-9]([A-Z0-9]{3,3}){1,1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
