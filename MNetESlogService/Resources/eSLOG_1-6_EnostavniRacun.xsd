			<xs:sequence>
				<xs:element ref="OpisPopusta" minOccurs="0"/>
				<xs:element ref="TipPopusta" minOccurs="0"/>
				<xs:element ref="OdstotekPopusta" minOccurs="0"/>
				<xs:element ref="ZnesekPopusta" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="OpisPopusta">
		<xs:annotation>
			<xs:documentation>Opis popusta</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="70"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="TipPopusta">
		<xs:annotation>
			<xs:documentation>Tip popusta (PP-popust na popust, VP-vzporedni popust)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:NMTOKEN">
				<xs:enumeration value="PP"/>
				<xs:enumeration value="VP"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="OdstotekPopusta">
		<xs:annotation>
			<xs:documentation>Odstotek popusta</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:float">
				<xs:pattern value="([0-9]|\+|\-|E|e|\.){0,10}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- e-SLOG 1.6 -->
	<xs:element name="ZnesekPopusta">
		<xs:annotation>
			<xs:documentation>Znesek popusta</xs:documentation>
